{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "portfolio.pure",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "Portfolio",
        "description": "Portfolio"
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": {
            "viewName": "portfolio.pure.view.Main",
            "type": "XML"
        },
        "dependencies": {
            "minUI5Version": "1.40.0",
            "libs": {
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "portfolio.pure.i18n.i18n"
                }
            }
        }
    }
}
